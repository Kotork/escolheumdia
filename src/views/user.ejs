<%- include('./partials/_header.ejs') %>
<link rel="stylesheet" href="styles/page-user.css">
<%- include('./partials/_navbar.ejs') %>

<div class="container--login">
  <%- include('./partials/_sideMenu.ejs') %>

  <div class="main--content">
    <% switch (data.page) {
      case 'Profile': %>
        <%- include('./partials/user/profile.ejs') %>
        <% break; %>
      <% case 'Cards': %>
        <%- include('./partials/user/card.ejs') %>
        <% break; %>
      <% case 'Bookings': %>
        <%= data.page %>
        <% break; %>
      <% case 'Invoices': %>
        <%= data.page %>
        <% break; %>
      <% default: %>
        <%- include('./partials/user/profile.ejs') %>
    <% } %>

    <% if (session) { %>
      <p> <%- JSON.stringify(session) %> </p>
    <% } %>
  </div>
</div>

<%- include('./partials/_footer.ejs') %>